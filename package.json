  "private": true,
  "dependencies": {
    "d3-dsv": "^1.0.10",
    "leaflet": "^1.2.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "rc-slider": "^8.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "5.0.1",
    "reactstrap": "^7.1.0",
    "recharts": "^1.0.0-beta.7",
    "request": "^2.87.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
